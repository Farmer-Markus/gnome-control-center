msgid "Device type:"
msgstr "Tipo de dispositivo:"

#: ../panels/color/color.ui.h:28
msgid "Manufacturer:"
msgstr "Fabricante:"

#: ../panels/color/color.ui.h:29
msgid "Model:"
msgstr "Modelo:"

